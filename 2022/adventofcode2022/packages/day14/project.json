{
  "name": "day14",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/day14/src",
  "targets": {
    "build": {
      "executor": "@adventofcode2022/aoc-plugin:build"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/day14/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "run-aoc": {
      "executor": "@adventofcode2022/aoc-plugin:run-aoc",
      "options": {
        "code": "packages/day14/src/lib/code.ts",
        "input": "packages/day14/src/lib/input"
      }
    }
  },
  "tags": []
}
